<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="WindowBackground">#23272E</Color>
    <Color x:Key="PanelBackground">#2C313A</Color>
    <Color x:Key="PanelBorder">#444950</Color>
    <Color x:Key="TextColor">#EEE</Color>
    <Color x:Key="InputBackground">#23272E</Color>
    <Color x:Key="InputBorder">#444950</Color>
    <Color x:Key="ButtonBackground">#3B82F6</Color>
    <Color x:Key="HyperlinkColor">#60AFFF</Color>
    <SolidColorBrush x:Key="WindowBackgroundBrush" Color="{StaticResource WindowBackground}" />
    <SolidColorBrush x:Key="PanelBackgroundBrush" Color="{StaticResource PanelBackground}" />
    <SolidColorBrush x:Key="PanelBorderBrush" Color="{StaticResource PanelBorder}" />
    <SolidColorBrush x:Key="TextColorBrush" Color="{StaticResource TextColor}" />
    <SolidColorBrush x:Key="InputBackgroundBrush" Color="{StaticResource InputBackground}" />
    <SolidColorBrush x:Key="InputBorderBrush" Color="{StaticResource InputBorder}" />
    <SolidColorBrush x:Key="ButtonBackgroundBrush" Color="{StaticResource ButtonBackground}" />
    <SolidColorBrush x:Key="HyperlinkBrush" Color="{StaticResource HyperlinkColor}" />
    <Style TargetType="Label">
        <Setter Property="Foreground" Value="{DynamicResource TextColorBrush}" />
    </Style>
    <Style TargetType="TextBox">
        <Setter Property="Background" Value="{DynamicResource InputBackgroundBrush}" />
        <Setter Property="Foreground" Value="{DynamicResource TextColorBrush}" />
        <Setter Property="BorderBrush" Value="{DynamicResource InputBorderBrush}" />
        <Setter Property="CaretBrush" Value="{DynamicResource TextColorBrush}" />
        <Setter Property="SelectionBrush" Value="{DynamicResource ButtonBackgroundBrush}" />
    </Style>
    <Style TargetType="PasswordBox">
        <Setter Property="Background" Value="{DynamicResource InputBackgroundBrush}" />
        <Setter Property="Foreground" Value="{DynamicResource TextColorBrush}" />
        <Setter Property="BorderBrush" Value="{DynamicResource InputBorderBrush}" />
        <Setter Property="CaretBrush" Value="{DynamicResource TextColorBrush}" />
        <Setter Property="SelectionBrush" Value="{DynamicResource ButtonBackgroundBrush}" />
    </Style>
    <Style TargetType="Button">
        <Setter Property="FontWeight" Value="Bold" />
        <Setter Property="Foreground" Value="White" />
        <Setter Property="Background" Value="{DynamicResource ButtonBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{DynamicResource ButtonBackgroundBrush}" />
    </Style>
    <Style TargetType="Hyperlink">
        <Setter Property="Foreground" Value="{DynamicResource HyperlinkBrush}" />
    </Style>
</ResourceDictionary>